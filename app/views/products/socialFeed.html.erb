		<a href="<%= ranking_path %>">
		<button class="vigme-social-header-button btn btn-primary btn-sm">
  		Discover Trendsetters
  		 </button>
		</a>
	</div>
</div>

<div id="product-list" class="no-margin row col-md-offset-1">

<%= render 'social_feed', products_for_each_user: @products_for_each_user, social_feed_profiles: @social_feed_profiles %>


</div>
<a id="more_products_wrap" style="display: none"><%= button_to 'More', {:controller => "products", :action => "socialFeed", :more => 1, :page => 2, :product_page => 2}, id: 'more_products', :method => :get, remote: true %></a>
