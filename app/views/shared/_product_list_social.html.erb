
<div class="vigme-social-profile-row row"> 
		<%= image_tag profile.avatar.url, class: "vigme-social-profile-image" %>

	<a class="vigme-social-name"><%= profile.name %></a>
</div>
<div class="vigme-social-product-group">
<% products.each do |product| %>
	<div class="vigme-product-outline-social"> 
		<%= link_to image_tag( product["image"] || product.image_s3_url, :class=> "vigme-product-social"), {:controller => 'products', :action => "showProductModal", :id => (product["id"] || product.id) }, remote: :true, 'data-toggle' => 'modal', 'data-target' => '#product_detail_modal' %>
	</div> 
  <% end %>
</div>

	