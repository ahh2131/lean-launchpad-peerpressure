<h1>Listing test_modules</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_modules.each do |test_module| %>
      <tr>
        <td><%= link_to 'Show', test_module %></td>
        <td><%= link_to 'Edit', edit_test_module_path(test_module) %></td>
        <td><%= link_to 'Destroy', test_module, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test module', new_test_module_path %>
